<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dangdiary"
    xmlns:tools="http://schemas.android.com/tools">
<!-- package 부분 내가 작성 ^^ ..-->
    <!--    안드로이드 4대 컴포넌트 (Activity, Service, Broadcast Receiver, Content Provider가 정의된 곳-->
    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.Dangdiary"
        tools:targetApi="31">

        <activity
            android:name=".Join"
            android:exported="false" />
        <activity
            android:name=".FoodRegister"
            android:exported="false" />
        <activity
            android:name=".BloodRegister"
            android:exported="false" />
        <activity
            android:name=".HomeMenu"
            android:exported="false" />
        <activity
            android:name=".BloodOrFood"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>